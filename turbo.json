{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "dev": {
      "cache": false,
      "persistent": true
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".nuxt/**", ".output/**"],
      "cache": true
    },
    "test": {
      "cache": false
    },
    "preview": {
      "cache": false,
      "persistent": true
    }
  }
}
